  },
  "homepage": "https://github.com/uabcFinalProject/under-the-hood#readme",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "antd": "^5.3.0",
    "moment": "^2.29.4",
    "react-router-dom": "^6.8.2"
  }
}
